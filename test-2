//函数   这里num1,num2的值都已确定无法改变
int Max(int x,int y)
{
	if (x > y)
		return x;
	else
		return y;
}
int main()
{
	int num1 = 10;
	int num2 = 20;
	int max = 0;
	max=Max(num1, num2);
	printf("max=%d\n", max);
	system("pause");
	return 0;
}
